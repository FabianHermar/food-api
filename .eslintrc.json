{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "space-before-function-paren": "off",
    "multiline-ternary": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-restricted-syntax": "off"
  }
}
